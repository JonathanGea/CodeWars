#include <stddef.h>

size_t get_count(const char *s)
{
  int c = 0;
  int count = 0;
  while (s[c] != '\0'){
    if (s[c] == 'a' || s[c] == 'e' || s[c] == 'i' || s[c] =='o' 
        || s[c] == 'u'){
       count++;
    }
     
    c++;
    }  
    return count;
}


// mengembalikan jumlah huruf kecil dari inputan
